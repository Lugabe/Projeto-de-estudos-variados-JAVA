package fundamentos;

public class AreaCircuferencia {

	public static void main(String[] args) {
		double raio = 3.4;         //Double é quando os numeros não são inteiros = 3,4 e 5,2, etc..
		final double pi = 3.14159; // final quer dizer que o valor não pode ser mudado 
		
		
		double area = pi*raio*raio;
		
		System.out.println(area);
		
		
	}
}
